					<ProtectedString name="Source">local meleevalue = script.Parent.Parent.LocalMelee.melee;
ball = script.Parent

function onHit(hit)
	if meleevalue.Value == false then
		return;
	end

	if hit.Parent == nil then
		return;
	end

	local human = hit.Parent:FindFirstChild("Humanoid");

	if human == nil then
		return;
	end

	human:TakeDamage(100);
	taghumanoid(human)
	wait(2)
	untagHumanoid(human)
end

function tagHumanoid(humanoid)
	local tag = ball:findFirstChild("creator")
	if tag ~= nil then
		local new_tag = tag:clone()
		new_tag.Parent = humanoid 
	end
end


function untagHumanoid(humanoid)
	if humanoid ~= nil then
		local tag = humanoid:findFirstChild("creator")
		if tag ~= nil then
			tag.Parent = nil
		end
	end
end 

